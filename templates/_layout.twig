<!DOCTYPE html>
<html lang="{{ craft.app.language }}">
  <head>
  {% block head %}
        <meta charset="utf-8"/>
         <title>{{ '5Merchants | ' ~ (docTitle | default('Welcome')) }}</title>
        <meta name="description" content="{{ metaDesc|default('5Merchants') }}">
        <meta name="viewport" content="width=device-width, initial-scale=1.0, maximum-scale=1.0, user-scalable=no, viewport-fit=cover" />
        <meta name="referrer" content="origin-when-cross-origin" />
        <link rel="icon" href="../uploads/images/favicon.png" type="image/x-icon" />
        <link rel="stylesheet" href="https://unpkg.com/boxicons@2.0.7/css/boxicons.min.css" />
        {% do craft.app.view.registerCssFile('@web/css/main.css') %}
    {% endblock %}
  </head>

  <body {{ attr(bodyAttributes ?? {}) }}>

    {% include '_partials/store_nav.twig' %}
    {% include '_partials/nav.twig' %}
    {% include '_partials/store_cart_summary.twig' %}
    
    
    <main>
      {% block content %}{% endblock %}
    </main>
 {% include '_partials/footer.twig' %}
    {% block scripts %}
       
        <script src="https://code.jquery.com/jquery-3.6.0.min.js"></script>
        <script src="https://cdn.jsdelivr.net/npm/bootstrap@5.0.2/dist/js/bootstrap.bundle.min.js" integrity="sha384-MrcW6ZMFYlzcLA8Nl+NtUVF0sA7MsXsP1UyJoMp4YLEuNSfAP+JcXn/tWtIaxVXM" crossorigin="anonymous"></script>
        <script type="module" src="/js/nav.js"></script>
    {% endblock %}

  </body>
</html>