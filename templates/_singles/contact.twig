{% extends "_layout.twig" %} 
{% set bodyAttributes ={ class: "contact" } %} 
{% set docTitle = entry.title %} 
{# {% set metaDesc = entry.metaDescription %}  #}

{% set page = craft.entries().section('contact').all() %} 
{% block content %} 
    {% for entry in page %} 

        <div class="conatined-content py-4  py-sm-vw-sm py-md-vw-xs ">
        <div class="contain d-flex flex-column flex-sm-row">
            <div class="col col-12 col-sm-4">
                {% if entry.title %} 
                    <h1>{{entry.title}}</h1>
                {% endif %} 
                {% if entry.bodyText %} 
                    <div>{{entry.bodyText}}</div>
                {% endif %}
            </div>
             <div class="col col-12 col-sm-8">

             </div>
        </div>
    </div>
    {% endfor %} 
{% endblock %}